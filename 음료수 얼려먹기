#include<bits/stdc++.h>
#include<cstdio>
using namespace std;
int N,M;
int visited[20][20];
int DFS(int x,int y){
	if(x<=-1 ||x>=N ||y<=-1|| y>=M) return 0;
	if(visited[x][y]!=1){
		visited[x][y]=1;
		DFS(x-1,y);
		DFS(x+1,y);
		DFS(x,y-1);
		DFS(x,y+1);
		return 1;
	}
	return 0;
}
int main(void){
	ios::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	cin>>N>>M;
	int result=0;
	
	for(int i=0; i<N; i++){
		for(int j=0; j<M; j++){
			scanf("%1d",&visited[i][j]);
		}
	}
	
	for(int i=0; i<N; i++){
		for(int j=0; j<M; j++){
			if(DFS(i,j)==1) result++;
		}
	}
	cout<<result;
}
