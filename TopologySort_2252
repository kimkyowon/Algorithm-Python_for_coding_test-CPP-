#include<bits/stdc++.h>
using namespace std;
int v,e; //노드의 개수와 간선의 개수 
int indegree[100001]; //각 노드에 대한 진입차수는 0으로 초기화 
vector<int> graph[100001]; // 각 노드에 연결된 간선 정보를 담기 위한 연결리스트 초기화 

void topologySort(){
	vector<int> result; //알고리즘 수행 결과를 담을 리스트 
	queue<int> q; //큐 라이브러리 사용 
	
	for(int i=1; i<=v; i++){ // 진입 차수가 0인 노드를 큐에 삽입 
		if(indegree[i]==0){
			q.push(i);
		}
	}
	while(!q.empty()){ // 큐가 빌 때 까지 
		int now=q.front(); // 순서는 상관 없으므로 큐에 먼저 들어간 순서대로 
		q.pop();
		result.push_back(now); // 결과 출력은 q에 들어갔다 나온 순서대로니까 바로바로 넣어줌 
		for(int i=0; i<graph[now].size(); i++){
			indegree[graph[now][i]]-=1; // 노드 하나를 제거했으므로 그와 연결되어있던 진입차수 1 감소 
			if(indegree[graph[now][i]]==0){ // 그것과 연결되어 있던 노드의 차수가 0이 됐다면  
				q.push(graph[now][i]); // 큐에 삽입 
			}
		}
	}
	for(int i=0; i<result.size(); i++){
		cout<<result[i]<<' ';
	}
}

int main(void){
	cin>>v>>e;
	for(int i=0; i<e; i++){
		int start,end;
		cin>>start>>end;
		graph[start].push_back(end);
		indegree[end]++;
	}
	topologySort();
}
